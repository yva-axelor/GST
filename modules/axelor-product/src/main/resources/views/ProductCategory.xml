<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">
  
  	<grid model="com.axelor.product.db.ProductCategory"
		title="Product Categories" name="product-category-grid">
		<field name="name"></field>
		<field name="code"></field>
		<field name="gstRate"></field>
	</grid>
	<form model="com.axelor.product.db.ProductCategory"
		title="Product Category" name="product-category-form">
		<panel>
			<field name="name"></field>
			<field name="code"></field>
			<field name="gstRate"></field>
		</panel>
	</form>
	
</object-views>